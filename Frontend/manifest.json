{
  "name": "CS2 Skin Price Checker",
  "version": "1.0.0",
  "description": "This is an extension to look up prices of CS2 Skins",
  "manifest_version": 3,
  "author": "Shashank Dahewar",
  "icons": {
    "16": "icons/16by16.png",
    "48": "icons/48by48.png",
    "128": "icons/128by128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "CS2 Skin Price Checker",
    "default_icon": {
      "16": "icons/16by16.png",
      "48": "icons/48by48.png",
      "128": "icons/128by128.png"
    }
  }
}
